<view><button style="margin:35rpx;" size="mini" type="primary" data-event-opts="{{[['tap',[['getTableData']]]]}}" bindtap="__e">今日送餐</button><button style="margin:35rpx;" size="mini" type="primary" data-event-opts="{{[['tap',[['getTmorData',['$event']]]]]}}" bindtap="__e">明日预订</button><button style="margin:35rpx;" size="mini" type="primary">指定日期</button><view style="margin-left:15rpx;">{{"今日需送："+total+"份"}}</view><view class="warp"><view class="box"><view class="title" style="margin:25rpx 15rpx;">今日订单</view><t-table bind:change="__e" vue-id="1" data-event-opts="{{[['^change',[['change']]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><t-tr vue-id="{{('2')+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><t-th vue-id="{{('3')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}">序号</t-th><t-th vue-id="{{('4')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}">姓名</t-th><t-th vue-id="{{('5')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}">食品</t-th><t-th vue-id="{{('6')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}">送餐时间</t-th></t-tr><block wx:for="{{tableList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><t-tr vue-id="{{('7-'+index)+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><t-td vue-id="{{('8-'+index)+','+('7-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{index+1}}</t-td><t-td vue-id="{{('9-'+index)+','+('7-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.name}}</t-td><t-td vue-id="{{('10-'+index)+','+('7-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.food}}</t-td><t-td vue-id="{{('11-'+index)+','+('7-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.time}}</t-td></t-tr></block><block wx:for="{{food}}" wx:for-item="items" wx:for-index="index" wx:key="index"><t-tr vue-id="{{('12-'+index)+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><t-td vue-id="{{('13-'+index)+','+('12-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">餐品：</t-td><t-td vue-id="{{('14-'+index)+','+('12-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{items.food_name+"："+items.nub+"个"}}</t-td><t-td vue-id="{{('15-'+index)+','+('12-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{"总计："+total+"个"}}</t-td></t-tr></block></t-table></view></view></view>