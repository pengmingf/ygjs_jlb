<view><view class="uni-list"><block wx:for="{{list}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view class="uni-list-cell" hover-class="uni-list-cell-hover"><view class="uni-list-cell-navigate uni-navigate-right uni-media-list "><view class="uni-media-list-logo"><block wx:if="{{showImg}}"><image src="{{value.img}}" data-event-opts="{{[['tap',[['detail',['$0'],[[['list','',key]]]]]]]}}" bindtap="__e"></image></block></view><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{value.name}}</view><view class="uni-media-list-text-bottom uni-ellipsis">{{value.phone}}</view></view><block wx:if="{{!value.qiandao}}"><view><button style="margin-left:10rpx;" type="primary" size="mini" data-event-opts="{{[['tap',[['daka',['$0'],[[['list','',key]]]]]]]}}" bindtap="__e">签到</button></view></block><block wx:if="{{value.qiandao}}"><view><button style="margin-left:10rpx;" type="primary" size="mini" disabled>已到</button></view></block><view class="uni-list-cell"><picker mode="multiSelector" data-id="{{value.id}}" data-index="{{key}}" value="{{multiIndex}}" range="{{allarray}}" data-event-opts="{{[['change',[['before_weight',['$event']]]]]}}" bindchange="__e"><button style="margin-left:10rpx;" type="default" size="mini">{{value.before_sp||"运动前"}}</button></picker></view><view class="uni-list-cell"><picker mode="multiSelector" data-id="{{value.id}}" data-index="{{key}}" value="{{multiIndex}}" range="{{allarray}}" data-event-opts="{{[['change',[['after_weight',['$event']]]]]}}" bindchange="__e"><button style="margin-left:10rpx;" type="warn" size="mini">{{value.after_sp||"运动后"}}</button></picker></view></view></view></block></view><view class="button"><button type="primary" size="default" data-event-opts="{{[['tap',[['add']]]]}}" bindtap="__e">添加用户</button></view><view class="button"><button type="warn" size="default" data-event-opts="{{[['tap',[['delet']]]]}}" bindtap="__e">删除用户</button></view></view>