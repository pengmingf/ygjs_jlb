<view class="content"><view class="input-group"><view class="input-row border"><text class="title">账号：</text><m-input bind:input="__e" vue-id="1" type="text" focus clearable placeholder="请输入手机号" value="{{account}}" data-event-opts="{{[['^input',[['__set_model',['','account','$event',[]]]]]]}}" bind:__l="__l"></m-input></view><view class="input-row border"><text class="title">密码：</text><m-input bind:input="__e" vue-id="2" type="password" displayable placeholder="请输入密码(6-16位)" value="{{password}}" data-event-opts="{{[['^input',[['__set_model',['','password','$event',[]]]]]]}}" bind:__l="__l"></m-input></view><view class="input-row border"><text class="title">姓名：</text><m-input bind:input="__e" vue-id="3" type="text" clearable placeholder="请输入姓名" value="{{name}}" data-event-opts="{{[['^input',[['__set_model',['','name','$event',[]]]]]]}}" bind:__l="__l"></m-input></view><view class="input-row border"><text class="title">性别：</text><radio-group class="uni-flex" style="margin-top:8rpx;" name="sex" data-event-opts="{{[['change',[['getSexValue',['$event']]]]]}}" bindchange="__e"><label><radio value="0" checked></radio>男</label><label><radio value="1"></radio>女</label></radio-group></view></view><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注册</button></view></view>